  <refnamediv>
    <refname>
       &dhpackage;
    </refname>
    <refpurpose>
       a KDE graphical front-end for taskjuggler
    </refpurpose>
  </refnamediv>
  <refsynopsisdiv>
    <cmdsynopsis>
      <command>
         &dhpackage;
      </command>
      <arg>
        <option>
          <replaceable>
             Qt-options
          </replaceable>
        </option>
      </arg>
      <arg>
